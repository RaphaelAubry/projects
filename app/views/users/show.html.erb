<h1><%= @user.name %></h1>
<% if @user.tasks.present? %>
  <table-1>
    <table-row>
      <table-cell>
        Name
      </table-cell>
    </table-row>
    <% @user.tasks.each do |task| %>
      <table-row>
        <table-cell>
          <%= link_to task.name, project_path(task.project) %>
        </table-cell>
      </table-row>
    <% end %>
  </table-1>
<% end %>

<%= link_to :projects, projects_path %>
<%= link_to :users, users_path %>
